
{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.ts",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        },
        {
            "src": "src/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.ts"
        }
    ]
}